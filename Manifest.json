{
    "name": "The Link Finder",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "An extension to Get the link of current tabs.",
    "icons": {
        "16": "icons/SS_16.png",
        "32": "icons/SS_32.png",
        "48": "icons/SS_48.png",
        "128": "icons/SS_128.png"
    },
    "action": {
        "default_popup": "Src/popup.html"
    },
    "permissions": [
        "activeTab",
        "storage",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
        "src/timer/timer.js"
      ]
        }
    ]
}
